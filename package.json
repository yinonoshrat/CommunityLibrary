{
  "name": "communitylibrary",
  "version": "1.0.0",
  "description": "Web app to manage a community library",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../api && npm install",
    "vercel-build": "cd frontend && npm install && npm run build"
  },
  "keywords": [
    "library",
    "community",
    "react",
    "vite",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.4",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.84.0",
    "@types/react-window": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "openai": "^6.9.1",
    "react-window": "^2.2.3",
    "sharp": "^0.34.5"
  }
}
