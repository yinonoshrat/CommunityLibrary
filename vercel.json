{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ],
  "github": {
    "silent": false
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "production": true
    }
  },
  "alias": ["dev.communitylibrary.com"]
}
